{
  "name": "LogScriber",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "../node_modules/.bin/rimraf dist",
    "build": "../node_modules/.bin/nest build",
    "format": "../node_modules/.bin/prettier --write \\\\\\\"server/src/**/*.ts\\\\\\\" \\\\\\\"test/**/*.ts\\\\\\\"",
    "start": "../node_modules/.bin/nest start",
    "start:dev": "../node_modules/.bin/nest start --watch",
    "start:debug": "../node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "../node_modules/.bin/eslint \\\"{src,apps,libs,test}/**/*.ts\\\" --fix",
    "test": "../node_modules/.bin/jest",
    "test:watch": "../node_modules/.bin/jest --watch",
    "test:cov": "../node_modules/.bin/jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "test:e2e": "../node_modules/.bin/jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/speech": "^4.4.0",
    "@grpc/proto-loader": "^0.5.6",
    "@mapbox/node-pre-gyp": "^1.0.1",
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/microservices": "^7.6.14",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/platform-socket.io": "^7.6.14",
    "@nestjs/platform-ws": "^7.6.14",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/websockets": "^7.6.14",
    "helmet": "^4.4.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "socket.io": "^4.0.0",
    "socket.io-redis": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "wrtc": "^0.4.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "0.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5",
    "webpack": "^5.27.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
